<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product Editor</title>
    <link rel="stylesheet" href="/styles/Admin.css">
    
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>ID</th>
            <th>Price</th>
            <th>Brief</th>
            <th>Image link</th>
            <th>Created At</th>
            <th>Updated At</th>
        </tr>
        <% datas.forEach(data=> { %>
            <tr>
                <td><%=  data.name %></td>
                <td><%=  data._id %></td>
                <td><%=  data.price %></td>
                <td><%=  data.brief %></td>
                <td><%=  data.img %></td>
                <td><%=  data.createdAt %></td>
                <td><%=  data.updatedAt %></td>
                <td><a class="delete" href="/api/Udata/delete/<%= data._id %>/?key=123456789">Delete</a></td> 
            </tr>
    
            <% }); %>


    </table>
</body>
</html>